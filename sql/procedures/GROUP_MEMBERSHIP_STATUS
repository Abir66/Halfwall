CREATE OR REPLACE FUNCTION "GROUP_MEMBERSHIP_STATUS" (Q_GROUP_ID IN NUMBER, Q_USER_ID IN NUMBER)
RETURN VARCHAR2
AS
	MEMBERSHIP VARCHAR2(255);
BEGIN
	SELECT STATUS INTO MEMBERSHIP FROM GROUP_MEMBERS
	WHERE GROUP_ID = Q_GROUP_ID AND USER_ID = Q_USER_ID;
	
	RETURN MEMBERSHIP;

EXCEPTION
	WHEN OTHERS THEN
		RETURN NULL;
	
END;